{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    [
      "styled-components", {
        "ssr": true,
        "displayName": true,
        "preprocess": false,
        "minify": true
      }
    ],
    [
      "module-resolver", {
        "root": ["./"],
        "alias": {
          "@": "./"
        }
      }
    ],
    ["@emotion"]
  ]
}